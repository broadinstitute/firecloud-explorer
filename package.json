{
  "name": "firecloud-explorer",
  "productName": "Firecloud Download Client",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "npm run lint && ng test --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rm -rf ./dist/",
    "build:travisci": "ng build --target production --aot --build-optimizer --vendor-chunk --extract-css --output-hashing all --stats-json --deploy-url /firecloud-explorer/ --base-href /firecloud-explorer",
    "ci": "npm run clean && ng lint && ng test --browser ChromeTravisCi --single-run --reporters spec --sourcemaps=false && npm run build:travisci",
    "electron": "electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0-rc.1",
    "@angular/cdk": "^2.0.0-beta.12",
