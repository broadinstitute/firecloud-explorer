{
  "name": "firecloud-explorer",
  "productName": "FireCloud Explorer",
  "author": "Broad Institute",
  "productDescription": "File explorer for FireCloud Workspaces to download and upload files",
  "description": "FireCloud Download Client Description",
  "maintainer": "Broad Institute.",
  "version": "0.1.0",
  "appVersion": "0.1.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && tsc main.ts --lib es2015",
    "test": "npm run lint && ng test --single-run",
    "lint": "ng lint",
    "e2e": "./node_modules/mocha/bin/mocha",
    "clean": "rm -rf ./dist/",
    "build:travisci": "ng build --target production --aot --build-optimizer --vendor-chunk --extract-css --output-hashing all --deploy-url /firecloud-explorer/ --base-href /firecloud-explorer",
    "ci": "npm run clean && ng lint && ng test --browser ChromeTravisCi --single-run --reporters spec --sourcemaps=false && npm run build:travisci",
    "electron": "electron ."
  },
  "private": true,
