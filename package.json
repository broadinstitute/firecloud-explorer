    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "ngx-bootstrap": "^1.9.3",
    "ngx-electron": "^1.0.3",
    "primeng": "^4.2.2",
    "rxjs": "^5.4.2",
    "uuid": "^3.1.0",
    "web-animations-js": "^2.2.5",
    "zone.js": "0.8.12"
